{
  "name": "therapy-navigation-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "generate-api-client": "npx openapi-typescript-codegen --input ../backend/docs/swagger.json --output ./src/api/generated --client fetch",
    "generate-websocket-types": "cd ../backend && go run scripts/generate-types.go ../frontend/src/types/websocket.ts",
    "generate-types": "npm run generate-api-client && npm run generate-websocket-types",
    "check-api": "test -f ../shared/api-spec.json || (echo 'ERROR: API spec not found. Run backend build first.' && exit 1)",
    "prebuild": "npm run generate-types",
    "build": "tsc && vite build",
    "build-only": "vite build",
    "build-safe": "./scripts/build-safe.sh",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-form": "^0.1.0",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@types/d3": "^7.4.3",
    "@types/react-router-dom": "^5.3.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "firebase": "^12.3.0",
    "framer-motion": "^12.23.7",
    "lucide-react": "^0.525.0",
    "next-themes": "^0.4.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-firebase-hooks": "^5.1.1",
    "react-router-dom": "^7.7.1",
    "reactflow": "^11.11.4",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.21.4",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "axios": "^1.6.7",
    "chalk": "^5.4.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "openapi-typescript": "^7.8.0",
    "openapi-typescript-codegen": "^0.25.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vite-plugin-checker": "^0.10.0"
  }
}
